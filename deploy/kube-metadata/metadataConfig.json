{
  "kubeClusterName": "vault",
  "serviceId":       "metadata",
  "environment":     "testing",

  
  "nats": 
  {
    "natsUseTLS":          "true",
    "natsUrl":             "tls://10.1.1.12:4222",
    "natsCredentialsFile": "metadata-nats-client-tls",   
    "natsCaCertFile":      "/etc/nats-ca-certs/ca.crt",
    "natsClientCertPath":  "/etc/nats-client-tls-certs/",
    "natsClientCertFile":  "/etc/nats-client-tls-certs/tls.crt",
    "natsClientKeyFile":   "/etc/nats-client-tls-certs/tls.key",
    "clientIdentity":      "metadata",
    "serviceId":           "metadata"  
  },
  
  "vault":
  {
    "vaultagentAddr":     "http://127.0.0.1:8100",
    "vaultAgentHost":     "127.0.0.1",
    "vaultAgentPort":     "8100",
    "appRoleSecretName":  "metadata-bao-approle",
    "appRoleName":        "metadata",
    "appRoleTokenPath":   "/home/bao/token",
    "secretIDRotationMs": "300000"
  },
  
  "keyExchange":
  {
    "keyRotationMinutes":    "240",
    "svcKeyExchStreamBase":  "METADATA_KEY_EXCHANGE",
    "svcKeyExchSubjectBase": "metadata.bundle-pull.svc-",
    "expiryMinutes":         "360",
    "consumerSubjectSvcId":  "metadata",
    "producerSubjectSvcIds": [ "watcher", "tester" ]
  },
  
  "servicesACL":
  {
    "aclConfigMapName": "services-acl",
    "aclNamespace":     "metadata",
    "encKeyExpiry":     "8",
    "rotatePriorHours": "5"
  },
  
  "transportKeys":
  [
    { "watcher": [ "tester" ] }
  ]
}
